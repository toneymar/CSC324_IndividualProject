<% provide(:title, 'Sign up') %>
<h1>Sign up</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @entries do |f| %>
      <%= f.label :date %>
      <%= f.text_field :date %>

      <%= f.label :acft_model %>
      <%= f.text_field :acft_model %>

      <%= f.label :acft_ident %>
      <%= f.text_field :acft_ident %>

      <%= f.label :from, "Confirmation" %>
      <%= f.text_field :from %>

      <%= f.submit "Create my account", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
